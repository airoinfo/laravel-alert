{
    "name": "airoinfo/laravel-alert",
    "description": "A simple PHP package to show alerts with the Laravel Framework and Vue",
    "keywords": ["laravel", "alert", "notifier"],
    "authors": [
        {
            "name": "user",
            "email": "user@example2.com.tw"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^5.6|^7.0|^8.0",
        "illuminate/support": "^5.5|^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Airo\\": "src/"
        },
        "files": [
            "src/Alert/functions.php"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Airo\\Alert\\AlertServiceProvider"
            ],
            "aliases": {
                "Alert": "Airo\\Alert\\AlertFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
