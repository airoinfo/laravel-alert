{
    "name": "airoinfo/laravel-alert",
    "description": "A simple PHP package to show alerts with the Laravel Framework and Vue",
    "keywords": ["laravel", "alert", "notifier"],
    "authors": [
        {
            "name": "user",
            "email": "user@example2.com.tw"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",
        "illuminate/support": "^5.5|^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Airo\\Alert\\": "src/"
        },
        "files": [
            "src/Alert/functions.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Airo\\Alert\\AlertServiceProvider"
            ],
            "aliases": {
                "Alert": "Airo\\Alert\\Alert"
            }
        }
    }
}
